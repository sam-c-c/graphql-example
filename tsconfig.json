{
    "compilerOptions": {
      "rootDir": "src",
      "outDir": "dist",
      "lib": ["ES6"],
      "target": "ESNext",
      "module": "Node16",
      "moduleResolution": "Node16",
      "esModuleInterop": true,
      "types": ["node"],
      "isolatedModules": true,
    },
    "include": [
       "src/**/*",
    ],
    "exclude": ["node_modules"]
  }